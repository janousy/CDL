{
  "pipeline": {
    "name": "bbc-test-store-label"
  },
  "description": "A pipeline that stores label from labelstudio to postgres",
  "transform": {
    "image": "jabalt/bbc-pach:prod",
    "cmd": [ "python3", "/write_db.py", "--inputpath", "/pfs/bbc-test-label", "--test" ]
  },
  "input": {
    "pfs": {
      "repo": "bbc-test-label",
      "glob": "/"
    }
  }
}